{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "App (debug mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file=config/dev/.env",
                "--web-browser-flag=--disable-web-security",
            ]
        },
        {
            "name": "App (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file=config/dev/.env",
            ]
        },
        {
            "name": "App (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file=config/dev/.env",
            ]
        }
    ]
}